              "$ref": "#/definitions/Error"
            }
          }
        }
      },
      "put": {
        "summary": "更新{{tag}}",
        "description": "通过ID更新{{tag}}",
        "tags": [
          "{{className}}"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "description": "更新{{tag}}的ID",
            "required": true
          },
          {
            "in": "body",
            "name": "body",
            "description": "数据更新的内容.",
            "required": true,
            "schema": {
              "$ref": "#/definitions/{{className}}"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "更新成功",
            "schema": {
              "type": "object",
              "properties": {
                "updatedAt": {
                  "description": "更新时间",
                  "type": "string",
                  "format": "date-time"
                },
                "objectId": {
                  "description": "objectId",
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "object not found",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
