        "tags": [
          "{{className}}"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "description": "{{tag}}的ID",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "返回{{tag}}数据",
            "schema": {
              "$ref": "#/definitions/{{className}}"
            }
          },
          "404": {
            "description": "object not found",
